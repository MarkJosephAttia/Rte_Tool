            </P-PORT-PROTOTYPE>
          </PORTS>
          <INTERNAL-BEHAVIORS>
            <SWC-INTERNAL-BEHAVIOR>
              <SHORT-NAME>Right_Door_InternalBehavior</SHORT-NAME>
            </SWC-INTERNAL-BEHAVIOR>
          </INTERNAL-BEHAVIORS>
        </APPLICATION-SW-COMPONENT-TYPE>
        <SWC-IMPLEMENTATION>
          <SHORT-NAME>Right_Door_Implementation</SHORT-NAME>
          <BEHAVIOR-REF DEST="SWC-INTERNAL-BEHAVIOR">/Dimmer/Right_Door/Right_Door_InternalBehavior</BEHAVIOR-REF>
        </SWC-IMPLEMENTATION>
      </ELEMENTS>
    </AR-PACKAGE>
  </AR-PACKAGES>
